    int age;
    double income;

    // Prompt user for age and income
    printf("Enter your age:\n ");
    scanf("%d", &age);
    printf("Enter your annual income (in Ksh):\n ");
    scanf("%lf", &income);

    // Check eligibility
    if (age >= 21 & income >= 21000) {
